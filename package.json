{
  "name": "ngx-device-detector",
  "version": "1.3.4",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p --tsconfig ./tsconfig.json -n ngx-device-detector -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start",
    "pre-release": "npm run lint && npm run build && npm run docs:build",
    "release": "npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KoderLabs/ngx-device-detector.git"
  },
  "author": {
    "name": "Ahsan Ayaz",
    "email": "ahsan.ubitian@gmail.com"
  },
  "keywords": [
    "angular",
    "device",
    "device detector",
    "device detection",
    "angular5",
    "angular6"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AhsanAyaz/ngx-device-detector.git/issues"
  },
  "devDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "2.8.7",
    "@types/node": "~10.1.0",
    "codelyzer": "~4.3.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-rollup": "^2.16.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "node-sass": "^4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.8",
    "opn": "^5.3.0",
    "protractor": "~5.3.2",
    "rollup": "^0.59.1",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.3.3",
    "systemjs": "^0.21.3",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  }
}
