{
  "name": "ngx-device-detector",
  "version": "1.4.5",
  "scripts": {
    "build": "ng-packagr -p ng-package.json && cp dist/bundles/ngx-device-detector.umd.js dist/ngx-device-detector.umd.js",
    "build:watch": "ng-packagr -p ng-package.json -w",
    "docs": "npm run docs:build",
    "docs:build": "node_modules/.bin/compodoc -p --tsconfig ./tsconfig.json -n ngx-device-detector -d docs --hideGenerator --theme material",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "node_modules/.bin/prettier --check src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "node_modules/.bin/concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "rimraf ./coverage && karma start",
    "pre-release": "npm run lint && npm run build && npm run docs:build",
    "release": "npm run pre-release && npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KoderLabs/ngx-device-detector.git"
  },
  "author": {
    "name": "Muhammad Ahsan Ayaz",
    "email": "ahsan.ubitian@gmail.com"
  },
  "keywords": [
    "angular",
    "device",
    "device detector",
    "device detection",
    "ngx-device-detector",
    "angular device detector",
    "angular5",
    "angular6",
    "angular7",
    "angular8"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AhsanAyaz/ngx-device-detector.git/issues"
  },
  "devDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngtools/webpack": "^8.3.2",
    "@types/jasmine": "^2.8.7",
    "@types/node": "~10.1.0",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "babel-polyfill": "^6.26.0",
    "codelyzer": "~4.3.0",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lite-server": "^2.3.0",
    "ng-packagr": "^5.1.0",
    "node-sass": "^4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.5.8",
    "opn": "^5.3.0",
    "prettier": "^2.0.5",
    "protractor": "~5.3.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.3.3",
    "source-map-loader": "^0.2.4",
    "systemjs": "^0.21.3",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "webpack": "^4.39.3",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0 <9.0.0 || ^9.0.0-0"
  }
}
